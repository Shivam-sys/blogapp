<div class="display-2">All Blogs</div>
<div class="container">
  <div class="row">
    <div class="col-sm-6">
      <h1>Tech</h1>
      {{#each techBlogs}}
        <li>
          <a href="/tech/{{this.slug}}">
            {{this.title}}
          </a>
        </li>
      {{/each}}
    </div>
    <div class="col-sm-6">
      <h1>Life</h1>
      {{#each lifeBlogs}}
        <li>
          <a href="/life/{{this.slug}}">
            {{this.title}}
          </a>
        </li>
      {{/each}}
    </div>
  </div>
</div>